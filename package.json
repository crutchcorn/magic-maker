{
  "name": "magicmaker",
  "version": "0.0.0-alpha.1",
  "description": "A un-opinionated static site generator for React",
  "type": "module",
  "scripts": {
    "dev": "node --loader ./loader/loader.js dev.ts",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "eslint **/*.{ts,tsx}",
    "format": "eslint **/*.{ts,tsx} --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "SSG",
    "React",
    "Static Site Generator"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.18.0",
    "@babel/traverse": "^7.18.0",
    "@testing-library/dom": "^8.13.0",
    "jsdom": "^19.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rehype-javascript-to-bottom": "^3.0.0",
    "rehype-parse": "^8.0.4",
    "rehype-stringify": "^9.0.3",
    "unified": "^10.1.2",
    "vitest": "^0.12.8"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.1.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --cache --fix"
  }
}
